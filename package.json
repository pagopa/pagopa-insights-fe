  "scripts": {
    "envconfig": "chmod +x envconfig.sh && . ./envconfig.sh && chmod +x version.sh && ./version.sh",
    "clean": "npm-run-all clean:*",
    "prestart": "npm-run-all envconfig",
    "start": "craco start",
    "start:local": "export $(grep -v '^#' .env.local) && ./envconfig.sh && ./version.sh && craco start",
    "prebuild": "npm-run-all envconfig",
    "build": "CI=false && craco build",
    "clean:build": "rimraf build",
    "lint": "eslint -f json -o eslint-report.json . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint:local": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "pretest": "npm-run-all envconfig",
    "test": "jest --coverage --ci --testResultsProcessor jest-sonar-reporter",
    "test:local": "jest --coverage --ci",
    "clean:api": "rimraf src/api/generated && rimraf openapi/generated",
    "generate:api": "wget https://raw.githubusercontent.com/pagopa/pagopa-mock-config-be/main/openapi/openapi.json -O ./openapi/api-docs.json && jq 'walk(if type == \"object\" and has(\"parameters\") then .parameters |= map(select(.name != \"X-Request-Id\")) else . end)' ./openapi/api-docs.json > ./openapi/api-docs.json.temp && mv ./openapi/api-docs.json.temp ./openapi/api-docs.json && yarn run clean:api && mkdirp openapi/generated && api-spec-converter --from=openapi_3 --to=swagger_2 openapi/api-docs.json > openapi/generated/swagger20.json && node openapi/scripts/api_fixPreGen.js && gen-api-models --api-spec openapi/generated/swagger20.json --out-dir src/api/generated --no-strict --request-types --response-decoders --client && node openapi/scripts/api_fixPostGen.js",
    "generate": "npm-run-all generate:*",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\""
  },
